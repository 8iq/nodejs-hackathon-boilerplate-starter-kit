{
  "name": "start-up-kit",
  "version": "0.0.2",
  "description": "Easy and Fast StartUp development kit",
  "private": true,
  "packageManager": "yarn@4.2.1",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
